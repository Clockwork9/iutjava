package edu.iut.app;

public class Option<cle extends Integer, val extends Integer, description extends String> {
	
	protected int cle ; 
	protected int val ; 
	protected String description ; 
	
	public Option(int cle, int val, String description) {
		this.cle = cle ; 
		this.val = val ; 
		this.description = description ; 
	}

}
